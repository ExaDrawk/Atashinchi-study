# Render.com Environment Variables
# Copy this file to .env and fill in your values

# Gemini AI API Key (Required for AI features)
GEMINI_API_KEY=your_gemini_api_key_here

# Node Environment
NODE_ENV=production

# Port (automatically set by Render.com)
PORT=3000

# Render.com specific
RENDER=true

# Authentication Settings
# Main user credentials (required)
AUTH_USERNAME=your_username_here
AUTH_PASSWORD=your_strong_password_here

# Additional users (optional, comma-separated)
# Format: "username1:password1,username2:password2"
AUTH_USERS=user1:pass1,user2:pass2

# Session security key (required for production)
SESSION_SECRET=your_session_secret_here
